#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#description
rdfs:description rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://data.dictionnairedesfrancophones.org/ontology/ddf#aboutEntity
ddf:aboutEntity rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf sioc:about ;
                owl:inverseOf ddf:hasItemAboutEntity ;
                rdfs:domain ddf:OnlineContribution ;
                rdfs:range ontolex:LexicalSense ;
                rdfs:comment "Discussion about any contributed entity"@en ,
                             "Discussion liée aux éléments de contribution"@fr ;
                rdfs:label "Note sur une entité"@fr ,
                           "aboutEntity"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#aboutEtymology
ddf:aboutEtymology rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sioc:about ;
                   owl:inverseOf ddf:hasItemAboutEtymology ;
                   rdfs:domain ddf:OnlineContribution ;
                   rdfs:range ontolex:LexicalEntry ;
                   rdfs:comment "Discussion about an Etymology"@en ,
                                "Discussion liée à l'histoire du mot"@fr ;
                   rdfs:label "aboutEtymology"@en ,
                              "note sur l'étymologie"@fr .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#aboutForm
ddf:aboutForm rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf sioc:about ;
              owl:inverseOf ddf:hasItemAboutForm ;
              rdfs:domain ddf:OnlineContribution ;
              rdfs:range ontolex:Form ;
              rdfs:comment "Discussion about a Form"@en ,
                           "Discussion liée à la forme"@fr ;
              rdfs:label "Note sur la forme"@fr ,
                         "aboutForm"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#aboutSense
ddf:aboutSense rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf sioc:about ;
               owl:inverseOf ddf:hasItemAboutSense ;
               rdfs:domain ddf:OnlineContribution ;
               rdfs:range ontolex:LexicalSense ;
               rdfs:comment "Discussion about a LexicalSense and usages"@en ,
                            "Discussion liée au sens et aux usages"@fr ;
               rdfs:label "Note sur le sens"@fr ,
                          "aboutSense"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#audioHasLocalisation
ddf:audioHasLocalisation rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:domain ddf:AudioRecording ;
                         rdfs:range ddf:Place .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#entryHasGlossary
ddf:entryHasGlossary rdf:type owl:ObjectProperty ;
                     rdfs:domain lexicog:Entry ;
                     rdfs:range ddf:Glossary ;
                     rdfs:comment "One of the meanings is included in a Glossary."@en ,
                                  "Un des sens de cette entrée appartient à un glossaire ."@fr ;
                     rdfs:label "entryHasGlossary"@en ,
                                "entrée dans un glossaire"@fr .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#entryHasSemanticRelationWith
ddf:entryHasSemanticRelationWith rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf ddf:entryHasGlossary ;
                                 owl:inverseOf ddf:semanticRelationOfEntry ;
                                 rdfs:domain lexicog:Entry ;
                                 rdfs:range ddf:SemanticRelation ;
                                 rdfs:comment "Indicate that an Entry has a semantic relation with another Entry or with a Sense"@en ,
                                              "Indique qu'une entrée entretient une relation sémantique avec une autre Entrée, une Entrée lexicale ou un Sens"@fr ;
                                 rdfs:label "entry has semantic relation with"@en ,
                                            "relation sémantique depuis une entrée"@fr .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#etymology
ddf:etymology rdf:type owl:ObjectProperty ;
              owl:inverseOf ddf:isEtymologyOf ;
              rdfs:domain ontolex:LexicalEntry ;
              rdfs:comment "Indication de l'étymologie."@fr ,
                           "Indication of the etymology."@en ;
              rdfs:label "etymology"@en ,
                         "étymologie"@fr .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#formHasLocalisation
ddf:formHasLocalisation rdf:type owl:ObjectProperty ;
                        rdfs:domain ontolex:Form ;
                        rdfs:range ddf:Place ;
                        rdfs:comment "Indicate a geographical restriction for the Form."@en ,
                                     "Lieu où est utilisée une forme (écrite ou sonore) du mot."@fr ;
                        rdfs:label "form has localisation"@en ,
                                   "restriction géographique pour la forme"@fr .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#formHasRecording
ddf:formHasRecording rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdfs:domain ontolex:Form ;
                     rdfs:range ddf:AudioRecording .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasConnotation
ddf:hasConnotation rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf ddf:hasSemanticProperty ,
                                      ontolex:usage ;
                   rdfs:domain ontolex:LexicalSense ;
                   rdfs:range ddf:Connotation ;
                   rdfs:comment "Indicate that a Sense has a connotation."@en ,
                                "Indication de la connotation du sens."@fr ;
                   rdfs:label "a une connotation"@fr ,
                              "has connotation"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasDomain
ddf:hasDomain rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf ddf:hasSemanticProperty ,
                                 ontolex:usage ;
              rdfs:domain ontolex:LexicalSense ;
              rdfs:range ddf:Domain ;
              rdfs:comment "Indicate that a Sense is part of a semantic domain."@en ,
                           "Indication d'un domaine sémantique pour un sens."@fr ;
              rdfs:label "a un domaine"@fr ,
                         "has domain"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasFormType
ddf:hasFormType rdf:type owl:ObjectProperty ;
                rdfs:domain ontolex:Form ;
                rdfs:range ddf:FormType ;
                rdfs:comment "Indicate the nature of the Form."@en ,
                             "Indique la nature de la forme."@fr ;
                rdfs:label "has form type"@en ,
                           "type de forme"@fr .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasFrequency
ddf:hasFrequency rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf ddf:hasSemanticProperty ,
                                    ontolex:usage ;
                 rdfs:domain ontolex:LexicalSense ;
                 rdfs:range lexinfo:Frequency ;
                 rdfs:comment "Indicate that a Sense has a frequency specification."@en ,
                              "Indication de fréquence d'usage d'un sens."@fr ;
                 rdfs:label "a une fréquence"@fr ,
                            "has frequency"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasGender
ddf:hasGender rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf ddf:hasGrammaticalProperty ;
              rdfs:domain ddf:InflectablePOS ,
                          ddf:MultiWordInflection ,
                          ontolex:MultiWordExpression ;
              rdfs:range lexinfo:Gender ;
              rdfs:comment "Genre grammatical d'un mot variable."@fr ,
                           "Indication of an inflectable nature caused by a gender specification."@en ;
              rdfs:label "genre du mot"@fr ,
                         "has gender"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasGlossary
ddf:hasGlossary rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf ddf:hasSemanticProperty ,
                                   ontolex:usage ;
                rdfs:domain ontolex:LexicalSense ;
                rdfs:range ddf:Glossary ;
                rdfs:comment "Indicate that a Sense is part of a glossary."@en ,
                             "Indique l'appartenance d'un sens à un glossaire."@fr ;
                rdfs:label "est dans un glossaire"@fr ,
                           "has glossary"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasGrammaticalConstraint
ddf:hasGrammaticalConstraint rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf ddf:hasSemanticProperty ,
                                                lemon:condition ;
                             rdfs:domain ontolex:LexicalSense ;
                             rdfs:range ddf:GrammaticalConstraint ;
                             rdfs:comment "Indicate that a Sense has a grammatical constraint to be used in."@en ,
                                          "Indique les contraintes syntaxiques d'usage pour un sens."@fr ;
                             rdfs:label "a une contrainte grammaticale"@fr ,
                                        "has grammatical constraint"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasGrammaticalProperty
ddf:hasGrammaticalProperty rdf:type owl:ObjectProperty ;
                           owl:inverseOf ddf:isGrammaticalPropertyOf ;
                           rdfs:domain ontolex:LexicalEntry ;
                           rdfs:range skos:Concept ;
                           rdfs:comment "meta property linking a lexical entry with any vocabulary concept"@en ,
                                        "méta propriété reliant une entrée lexicale avec n'importe quel concept de vocabulaire contrôlé"@fr ;
                           rdfs:label "a pour propriété grammaticale"@fr ,
                                      "has grammatical property"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasInflection
ddf:hasInflection rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf ddf:hasGrammaticalProperty ;
                  owl:inverseOf ddf:isInflectionOf ;
                  rdfs:domain ddf:InflectablePOS ;
                  rdfs:range ddf:Inflection ;
                  owl:propertyDisjointWith ddf:hasVerbalInflection ;
                  rdfs:comment "Indication of the inflected form of a Word."@en ,
                               "Indique les formes fléchies d'un mot."@fr ;
                  rdfs:label "a une flexion"@fr ,
                             "has inflection"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasItemAboutEntity
ddf:hasItemAboutEntity rdf:type owl:ObjectProperty ;
                       rdfs:domain ontolex:LexicalSense ;
                       rdfs:range ddf:OnlineContribution ;
                       rdfs:comment "Indicate a discussion about the contributed entity"@en ,
                                    "Indique une discussion à propos d'une contribution."@fr ;
                       rdfs:label "a une note sur l'entité"@fr ,
                                  "has discussion about Entity"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasItemAboutEtymology
ddf:hasItemAboutEtymology rdf:type owl:ObjectProperty ;
                          rdfs:domain ontolex:LexicalEntry ;
                          rdfs:range ddf:OnlineContribution ;
                          rdfs:comment "Indicate a discussion about the etymology of a LexicalEntry."@en ,
                                       "Indique une discussion à propos de l'histoire d'une entrée lexicale."@fr ;
                          rdfs:label "a une note sur l'étymologie"@fr ,
                                     "has item about etymology"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasItemAboutForm
ddf:hasItemAboutForm rdf:type owl:ObjectProperty ;
                     rdfs:domain ontolex:Form ;
                     rdfs:range ddf:OnlineContribution ;
                     rdfs:comment "Indicate a discussion about the Form of a LexicalEntry."@en ,
                                  "Indique une discussion à propos de la forme d'une entrée lexicale."@fr ;
                     rdfs:label "a une note sur la forme"@fr ,
                                "has item about form"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasItemAboutSense
ddf:hasItemAboutSense rdf:type owl:ObjectProperty ;
                      rdfs:domain ontolex:LexicalSense ;
                      rdfs:range ddf:OnlineCabouSontribution ;
                      rdfs:comment "Indicate a discussion about the sense and usages."@en ,
                                   "Indique une discussion à propos du sens et des usages."@fr ;
                      rdfs:label "a une note sur le sens"@fr ,
                                 "has item about Sense"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasLexicographicResource
ddf:hasLexicographicResource rdf:type owl:ObjectProperty ;
                             owl:inverseOf lexicog:entry ;
                             rdfs:domain lexicog:Entry ;
                             rdfs:range lexicog:LexicographicResource ;
                             rdfs:comment "Document d'origine de ce contenu"@fr ,
                                          "Indicate the source document"@en ;
                             rdfs:label "a pour origine"@fr ,
                                        "has lexicographic resource"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasLocalisation
ddf:hasLocalisation rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf ddf:hasSemanticProperty ,
                                       ontolex:usage ;
                    rdfs:domain ontolex:LexicalSense ;
                    rdfs:range ddf:Place ;
                    rdfs:comment "Indicate that a Sens has a localisation restriction."@en ,
                                 "Spécification d'une zone géographique pour un sens."@fr ;
                    rdfs:label "a une restriction géographique"@fr ,
                               "has localisation"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasMood
ddf:hasMood rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf ddf:hasGrammaticalProperty ;
            rdfs:domain ddf:VerbalInflection ;
            rdfs:range lexinfo:Mood ;
            rdfs:comment "Forme fléchie d'un verbe selon le mode."@fr ,
                         "Indication of an inflected nature caused by a mood caracterisation."@en ;
            rdfs:label "flexion en mode"@fr ,
                       "has mood"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasNumber
ddf:hasNumber rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf ddf:hasGrammaticalProperty ;
              rdfs:domain ddf:InflectablePOS ,
                          ddf:MultiWordInflection ,
                          ontolex:MultiWordExpression ;
              rdfs:range lexinfo:Number ;
              rdfs:comment "Indication of the number value of an inflectable term."@en ,
                           "Indique le nombre du mot."@fr ;
              rdfs:label "a un nombre"@fr ,
                         "has number"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasPartOfSpeech
ddf:hasPartOfSpeech rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf ddf:hasGrammaticalProperty ;
                    rdfs:domain ontolex:Word ;
                    rdfs:range lexinfo:PartOfSpeech ;
                    rdfs:comment "Indicate the type for a Word."@en ,
                                 "Indique la classe d'un mot."@fr ;
                    rdfs:label "a une classe de mot"@fr ,
                               "has part of speech"@en .

###  http://data.dictionnairedesfrancophones.org/ontology/ddf#equivalentPOSType
ddf:equivalentPOSType rdf:type owl:ObjectProperty ;
                    rdfs:domain skos:Concept ;
                    rdfs:comment "Classe de mot équivalente pour un type de locution. Utile pour le voc MultiWordType"@fr ;
                    rdfs:label "classe de mot équivalente"@fr .

###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasPerson
ddf:hasPerson rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf ddf:hasGrammaticalProperty ;
              rdfs:domain ddf:VerbalInflection ;
              rdfs:range lexinfo:Person ;
              rdfs:comment "Forme fléchie d'un verbe selon la personne."@fr ,
                           "Indication of an inflected nature caused by a person caracterisation."@en ;
              rdfs:label "flexion en personne"@fr ,
                         "has person"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasRegister
ddf:hasRegister rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf ddf:hasSemanticProperty ,
                                   ontolex:usage ;
                rdfs:domain ontolex:LexicalSense ;
                rdfs:range lexinfo:Register ;
                rdfs:comment "Indicate that a Sense has a register."@en ,
                             "Indication du registre d'usage lié à un sens."@fr ;
                rdfs:label "a un registre"@fr ,
                           "has register"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasReportingRating
ddf:hasReportingRating rdf:type owl:ObjectProperty ;
                       owl:inverseOf ddf:reportedItem ;
                       rdfs:domain owl:Thing ;
                       rdfs:range ddf:ReportingRating ;
                       rdfs:label "Has reporting rating"@en ,
                                  "Vote de signalement"@fr .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasSemanticProperty
ddf:hasSemanticProperty rdf:type owl:ObjectProperty ;
                        owl:inverseOf ddf:isSemanticPropertyOf ;
                        rdfs:domain ontolex:LexicalSense ;
                        rdfs:range skos:Concept ;
                        rdfs:comment "meta property linking a lexical sense with any vocabulary concept"@en ,
                                     "méta propriété reliant un sens avec n'importe quel concept de vocabulaire contrôlé"@fr ;
                        rdfs:label "a pour propriété sémantique"@fr ,
                                   "has semantic property"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasSociolect
ddf:hasSociolect rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf ddf:hasSemanticProperty ,
                                    ontolex:usage ;
                 rdfs:domain ontolex:LexicalSense ;
                 rdfs:range ddf:Sociolect ;
                 rdfs:comment "Indicate that a Sense is typical of a sociolect."@en ,
                              "Indication de l'appartenance d'un sens à un sociolecte."@fr ;
                 rdfs:label "est dans un sociolecte"@fr ,
                            "has sociolect"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasSuppressionRating
ddf:hasSuppressionRating rdf:type owl:ObjectProperty ;
                         owl:inverseOf ddf:suppressedItem ;
                         rdfs:domain owl:Thing ;
                         rdfs:range ddf:SuppressionRating ;
                         rdfs:label "Vote de suppression"@fr ,
                                    "has Suppression Rating"@en ,
                                    "has suppression rating"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasTemporality
ddf:hasTemporality rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf ddf:hasSemanticProperty ,
                                      ontolex:usage ;
                   rdfs:domain ontolex:LexicalSense ;
                   rdfs:range lexinfo:TemporalQualifier ;
                   rdfs:comment "Indicate that a Sense has a temporality."@en ,
                                "Indication de la modernité de l'usage d'un sens."@fr ;
                   rdfs:label "a une temporalité"@fr ,
                              "has temporality"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasTense
ddf:hasTense rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf ddf:hasGrammaticalProperty ;
             rdfs:domain ddf:VerbalInflection ;
             rdfs:range lexinfo:Tense ;
             rdfs:comment "Forme fléchie d'un verbe selon le temps."@fr ,
                          "Indication of an inflected nature caused by a tense caracterisation."@en ;
             rdfs:label "flexion en temps"@fr ,
                        "has tense"@en .


###  http://data.dictionnairedesfrancophones.org/ontology/ddf#hasTextualGenre
ddf:hasTextualGenre rdf:type owl:ObjectProperty ;
